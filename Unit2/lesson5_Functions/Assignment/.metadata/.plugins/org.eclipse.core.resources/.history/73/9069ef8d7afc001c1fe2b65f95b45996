#include <stdio.h>

int main()
{

	int num,f=1,i;

	printf("Enter an positive integer: ");
	fflush(stdout);
	scanf("%d",&num);

	if (num<=0)
	{
		printf("It is not an positive num.\nPlease re_enter an positive integer: ");
		fflush(stdout);
		scanf("%d",&num);
	}

	for(num>1;num--;)
	{
		f*=num;

	}

	printf("Factorial of %d = %d",num,f);

}
