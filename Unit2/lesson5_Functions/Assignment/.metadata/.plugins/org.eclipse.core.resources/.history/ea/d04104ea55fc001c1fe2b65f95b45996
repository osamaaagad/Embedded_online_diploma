#include <stdio.h>


void primenum(int i,int j)
{
	int k;
	for(k=i-1;k>=i&&k<j;k++)
	{
		k++;

		printf("Prime numbers between %d and %d are: %d ",i,j,k);

	}

}

int main ()
{

	int x,y;

	printf("Enter two numbers(intervals): ");
	fflush(stdout); fflush(stdin);
	scanf("%d%d",&x,&y);



	primenum(x,y);



	return 0;



}
