#include <stdio.h>


void primenum(int i,int j)
{
	int k;
	for(k=i+1;k<j;k+=2)
	{
		printf("%d",k);
			fflush(stdout);

	}


}

int main ()
{

	int x,y;

	printf("Enter two numbers(intervals): ");
	fflush(stdout); fflush(stdin);
	scanf("%d%d",&x,&y);

	printf("Prime numbers between %d and %d are: ",x,y);

	primenum(x,y);



	return 0;



}
