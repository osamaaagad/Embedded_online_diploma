#include <stdio.h>

int count_of_ones(int num)
{
	int i,j;

	printf("\nthe count of ones in binary of number: ");
		fflush(stdout);

		for (i=31;i>=0;i--)

		{

			j=num>>i;

			if (j&1)
			{
				printf("1");
			}




}
int main()
{

	int num,i,j;

	printf("Enter an integer: ");
	fflush(stdout);
	scanf("%d",&num);



}
