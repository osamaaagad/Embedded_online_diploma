#include <stdio.h>

int sum_numbers(int x,int y)
{

	x++;
	y+=x;

	if(x<5){

		sum_numbers(x,y);
	}

	printf("the sum of numbers from 1 to 100 = %d",y);
	return 0;

}

int main()
{
	int i=0,sum=0,k;

	k=sum_numbers(i,sum);




	return 0;
}
