#include <stdio.h>
#include <math.h>


void func(int num)
{

	double i;

	i=sqrt(num);

	printf("Squareroot of (%d)= %3lf",num,i);

}

int main()
{
	int num;

		printf("Enter a number: "); fflush(stdout);
		scanf("%d",&num);

		func(num);

	return 0;


}
