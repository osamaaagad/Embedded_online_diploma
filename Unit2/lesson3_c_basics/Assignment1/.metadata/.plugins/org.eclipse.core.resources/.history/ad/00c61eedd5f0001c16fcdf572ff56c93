#include <stdio.h>

int main()
{

	float a,b,x,y;

	printf("Enter value of a:");
	fflush(stdout);
	scanf("%f",&a);
	x=a;

	printf("\nEnter value of b:");
	fflush(stdout);
	scanf("%f",&b);
	y=b;

	printf("After swapping, value of a = %f",y);

	printf("After swapping, value of b = %f",x);

}
