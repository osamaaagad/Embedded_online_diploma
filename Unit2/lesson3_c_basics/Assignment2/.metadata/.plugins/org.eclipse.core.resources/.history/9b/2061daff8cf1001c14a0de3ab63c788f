#include <stdio.h>

int main()
{

	int vnum=1,Factorial=0,num;

	printf("Enter an integer: ");
	fflush(stdout);
	scanf("%d",&num);

	if(num<0)
	{
		printf("Error!!! Factorial of negative number doesn't exist");
	}

	else
	{

		while(vnum<=num)
		{
			Factorial=Factorial*vnum;
			vnum++;
		}

		printf("Factorial=%d",Factorial);


	}

	return 0;
}
