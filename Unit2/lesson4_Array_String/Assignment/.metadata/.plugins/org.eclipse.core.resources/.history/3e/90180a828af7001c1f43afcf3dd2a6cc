#include <stdio.h>
#include <string.h>

int main ()
{

	char text[100];
	int length,i,x;

	printf("Enter a string: ");
	fflush(stdout);
	gets(text);

	strlen[text]=x;
	length=x-1;

	for(i=length;text[i]!=0;i--)
	{
		printf("\nReverse string is: %c",text);
	}



}
