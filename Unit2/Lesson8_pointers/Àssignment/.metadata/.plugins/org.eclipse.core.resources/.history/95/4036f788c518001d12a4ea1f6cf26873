#include <stdio.h>
#include <string.h>

int main()
{

	char str[100];
	char* p;
	int i;

	prinf("Input a string: "); fflush(stdout);
	gets(str);
	prinf("\nReverse of the string is: "); fflush(stdout);

	i=strlen(str);
	p=&str[i-1];

	while(i>0)
	{

		printf("%c",*p);
		p--;
		i--;

	}
return 0;
}
