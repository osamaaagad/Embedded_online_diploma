#include <stdio.h>
#include <stdlib.h>

int main()
{

	float x,y,product;

	printf("Enter Two Numbers: ");
	scanf("%f %f" ,&x,&y);

	product=x*y;

	prinf("\nproduct= %f" ,product);

	return 0;
}

